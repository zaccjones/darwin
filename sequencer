sequence = []
def nucleotide(text):
  for x in text:
    if x == "T":
      sequence.append(x)
    elif x == "G":
      sequence.append(x)
    elif x == "A":
      sequence.append(x)
    elif x == "C":
      sequence.append(x)
text = "SAMPLETEXT"
nucleotide(text)
genome = "".join(sequence)
print genome
